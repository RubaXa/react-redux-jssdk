{
  "name": "react-redux-jssdk",
  "version": "0.1.0",
  "description": "Работа с JSSDK поверх React, плюс высякие хелперы",
  "author": "RubaXa <ibnRubaXa@gmail.com>",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage",
    "ts-build": "tsc",
    "git-clean": "git clean -f",
    "prepublish": "npm run test && npm run ts-build",
    "postpublish": "npm run git-clean"
  },
  "files": [
    "*.js",
    "*.d.ts",
    "src/*/*.js",
    "src/*/*.d.ts"
  ],
  "devDependencies": {
    "@types/jest": "^19.2.3",
    "@types/react-dom": "^15.5.5",
    "jest": "^20.0.3",
    "raf": "^3.4.0",
    "react-dom": "^16.0.0",
    "ts-jest": "^20.0.4",
    "typescript": "^2.4.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "\\.tests\\.tsx?$",
    "transformIgnorePatterns": [],
    "setupTestFrameworkScriptFile": "<rootDir>/__jest__/extensions.ts",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  },
  "dependencies": {
    "@perf-tools/balancer": "^0.2.1",
    "@types/enzyme": "^2.8.9",
    "@types/react": "^16.0.7",
    "@types/react-dom": "^15.5.5",
    "@types/react-redux": "^5.0.12",
    "@types/redux": "^3.6.0",
    "enzyme": "^3.0.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2"
  }
}
